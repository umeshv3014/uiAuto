<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>
		<listener class-name="com.myStore.utility.ListenerClass"></listener>
	</listeners>
	<groups>
		<run>
			<include name="SmokeTest" />
		</run>
	</groups>
	<test name="Test">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.myStore.testing.Car_loan" />
			<class name="com.myStore.testing.Home_loan" />
			<class name="com.myStore.testing.Personal_loan" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->